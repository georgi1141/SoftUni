
    <header>
           <nav>
            <ul class="navigation">
                <li><a href="/">Home Page</a></li>
                <li><a href="/addBreed">Add Breed</a></li>
                <li><a href="/addCat">Add Cat</a></li>
            </ul>
        </nav>
        <h1>Cat Shelter</h1>
    </header>
    <main>
        <form method="POST" class="cat-form">
            <h2>Edit Cat</h2>
            <label for="name">Name</label>
            <input name="name" type="text" id="name" value={{data.name}}>
            <label for="description">Description</label>
            <textarea name="description" id="description">{{data.description}}</textarea>
            <label for="image">Image</label>
            <input name="image" type="text" id="image" value={{data.image}}>
            <label for="group">Breed</label>
            <input name="breed" type="text" id="group" value={{data.breed}}>
           
            <button>Edit Cat</button>
        </form>
    </main>
 